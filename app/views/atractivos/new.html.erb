<h1>Nuevo Atractivo</h1>

<%= render 'form' %>

<%= link_to '/atractivos/?idCanton='+session[:id_canton], class: "btn btn-primary noVisited noHover" do %>
    
  <i class="glyphicon glyphicon-chevron-left"> Regresar</i>
<% end %>
<div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 5%;">
    0%
  </div>
</div>
<style>
  a.noVisited:visited {color: #ffffff;}
  a.noHover:hover {
    color: #ffffff;
    text-decoration: none;
  }
</style>
